<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="price.bit.com.quickscanner_qr.MainActivity">

    <Button
        android:id="@+id/start_s"
        android:layout_width="173dp"
        android:layout_height="51dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="81dp"
        android:layout_marginStart="81dp"
        android:text="@string/scan"
        tools:ignore="MissingConstraints"
        android:theme="@style/MyButton"
        android:gravity="center"
        tools:layout_editor_absoluteX="94dp"
        tools:layout_editor_absoluteY="29dp" />

    <Button
        android:id="@+id/start_g"
        android:layout_width="173dp"
        android:layout_height="51dp"
        android:gravity="center"
        android:layout_alignLeft="@+id/start_s"
        android:layout_alignStart="@+id/start_s"
        android:layout_below="@+id/start_s"
        android:layout_marginTop="20dp"
        android:text="@string/gen"
        android:theme="@style/MyButton"
        />


    <ListView
        android:paddingTop="120dp"
        android:id="@android:id/list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/start_g"/>


    <!--com.google.android.gms.ads.AdView
        xmlns:ads="http://schemas.android.com/apk/res-auto"
